  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@types/node": "^9.6.5",
    "@types/webpack": "^4.1.3",
    "@types/webpack-dev-server": "^2.9.4",
    "chokidar": "^2.0.3",
    "chokidar-cli": "^1.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^4.2.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "^2.8.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
